<!doctype html>

<html>
<head>
	<title>onTrack</title>	
	<meta charset="utf-8">	
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/view.css" rel="stylesheet">  
	<link href="/css/topBar.css" rel="stylesheet">   

	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Roboto:100,300" rel="stylesheet">
</head>

<body>
	  <!-- Main Top Bar -->
	<div class="topBar">
    	<a href="loggedIn" id="backButton">back</a>
    	<center><h1 id="yourRoutine">Your Routine</h1></center>
    	<div id="backArrow">
      		<img src="images/leftArrowGray.png" class="img-responsive"></img>
    	</div>
    	<button type="button" class="saveButton">save</button>
    </div>
	<div class="routine1"> <center><h1 id="routineTitle">Routine 1</h1></center>
	{{#each Routine1}}	
		<div class="Routine" id="routine1">
			<center><p> Task name: {{name}}</p>
			<p1> Time allowed: {{time}} minutes</p1> </center>
		</div>
	{{/each}}
	</div>
	<div class="routine2"> <center><h1 id="routineTitle">Routine 2</h1></center>
	{{#each Routine2}}
		<div class="Routine" id="routine2">	
			<center><p> Task name: {{name}}</p>
			<p1> Time allowed: {{time}} minutes</p1></center>
		</div>
	{{/each}}
	</div>
</body>
</html>